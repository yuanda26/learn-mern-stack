{
  "name": "devconnector",
  "version": "1.0.0",
  "description": "Social network for developers",
  "engines": {
    "node": "8.11.x",
    "npm": "5.6.x",
    "yarn": "1.9.x"
  },
  "main": "server.js",
  "scripts": {
    "test":"jest",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "concurrently": "^4.0.1",
    "create-react-app": "^1.5.2",
    "express": "^4.16.3",
    "global": "^4.3.2",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.12",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "nodemon": "^1.17.5",
    "superagent": "^3.8.3",
    "supertest": "^3.3.0"
  }
}
